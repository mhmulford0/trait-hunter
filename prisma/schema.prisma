// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./hunter.db"
}

model Lil {
  accessory       String
  backgroundColor String
  blockNumber     Int     @id
  glasses         String?
  head            String
}

model Alerts {
  id      Int      @id @default(autoincrement())
  content String
  added   DateTime @default(now())
}
